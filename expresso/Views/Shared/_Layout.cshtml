@using EXPRESSO.Models;

<!doctype html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="description" content="Expresso - Cafe HTML Template">
    <meta name="keywords" content="bootWeb, cafe, gallery, personal, developer, designer, onepage, clean, modern, expresso, web">
    <meta name="author" content="Mehul Prajapati">
    <meta name="apple-mobile-web-app-capable" content="yes">
    <title>Expresso - Cafe HTML Template</title>
    <!-- Favicon -->
    <link rel="shortcut icon" href="~/Public/expresso/images/favicon.png" type="image/png">
    <link rel="apple-touch-icon-precomposed" href="~/Public/expresso/images/favicon.png">
    <!-- Google Fonts -->
    <link href="https://fonts.googleapis.com/css?family=Open%20Sans:400,600,800%7CCourgette%7CCrete+Round" rel="stylesheet">
    <!-- Bootstrap -->
    <link href="~/Public/vendor/bootstrap/css/bootstrap.min.css" rel="stylesheet" />
    <!-- Animation -->
    <link href="~/Public/vendor/animate/animate.min.css" rel="stylesheet" />
    <!-- OWL Carousel -->
    <link href="~/Public/vendor/owl-carousel/owl.carousel.min.css" rel="stylesheet" />
    <!-- Font Awesome -->
    <link href="~/Public/vendor/font-awesome/css/font-awesome.min.css" rel="stylesheet" />
    <!-- Magnific Popup -->
    <link href="~/Public/vendor/magnific-popup/magnific-popup.css" rel="stylesheet" />
    <!-- Theme CSS -->
    <link href="~/Public/expresso/css/style.css" rel="stylesheet" />
    <link href="~/Public/expresso/css/responsive.css" rel="stylesheet" />
    <!--[if lt IE 9]>
    <script src="//oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
    <script src="//oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<!-- ========== Body Starts ========== -->
<body id="body">
    <!-- ========== Preloader ========== -->
    <div class="preloader">
        <div class="spinner">
            <div class="rect1"></div>
            <div class="rect2"></div>
            <div class="rect3"></div>
            <div class="rect4"></div>
            <div class="rect5"></div>
        </div>
    </div>
    <!-- ========== End of Preloader ========== -->
    <!-- ========== NavBar ========== -->
    <nav id="nav" class="navbar custom-navbar navbar-fixed-top mob-scroll">
        <div class="container">
            <!-- NAVBAR HEADER -->
            <div class="navbar-header">
                <button class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon icon-bar"></span>
                    <span class="icon icon-bar"></span>
                    <span class="icon icon-bar"></span>
                </button>
                <!-- lOGO TEXT HERE -->

                <a href="index-2.html" class="navbar-brand"><img src="~/Public/expresso/images/logo.png" class="img-responsive" alt="image"> <p>Ex<span class="contrast">press</span>o</p></a>
            </div>
            <!-- NAVIGATION LINKS -->
            <div class="collapse navbar-collapse">
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="#home" class="active">Home</a></li>
                    <li><a href="#about">About</a></li>
                    <li><a href="#service">Service</a></li>
                    <li><a href="#gallery">Gallery</a></li>
                    <li><a href="#blog">Blog</a></li>
                    <li><a href="#contact">Contact</a></li>
                </ul>
            </div>
        </div>
    </nav>

    @RenderBody();

    <footer>
        <div class="container">
            <div class="row">
                <div class="col-xs-12 col-md-3 col-lg-3 single-footer">
                    <h3>Newsletter</h3>
                    <p>@ViewBag.Newsletter.Title</p><br>
                    <form id="news-form" method="post" action="#">
                        <div class="input-group">
                            <input placeholder="Enter Your Email" class="form-control" type="text" required>
                            <span class="input-group-btn">
                                <button type="submit" class="btn news-btn">GO!</button>
                            </span>
                        </div>
                    </form>
                    <div class="col-xs-6 col-sm-6 col-md-6">
                        <h2 class="counter contrast">@ViewBag.Newsletter.Num1</h2>
                        <h4>@ViewBag.Newsletter.Content1</h4>
                    </div>
                    <div class="col-xs-6 col-sm-6 col-md-6">
                        <h2 class="counter contrast">@ViewBag.Newsletter.Num2</h2>
                        <h4>@ViewBag.Newsletter.Content2</h4>
                    </div>
                </div>
                <div class="col-xs-12 col-md-3 col-lg-3 single-footer">
                    <h3>Working Hours</h3>
                    <p>@ViewBag.WorkingHours.Title</p><br> <!-- Edit description -->
                    <p><i class="fa fa-clock-o"></i> <span>@ViewBag.WorkingHours.day1</span> @ViewBag.WorkingHours.hour1</p>
                    <p><i class="fa fa-clock-o"></i> <span>@ViewBag.WorkingHours.day2</span> @ViewBag.WorkingHours.hour2</p>
                    <p><i class="fa fa-clock-o"></i> <span>@ViewBag.WorkingHours.day3</span> @ViewBag.WorkingHours.hour3</p>
                </div>
                <div class="col-xs-12 col-md-3 col-lg-3 single-footer">
                    <h3>Call Us</h3>
                    <p>@ViewBag.callus.Title</p><br> <!-- Edit description -->
                    <p><i class="fa fa-map-marker" aria-hidden="true"></i> <span>Address:</span> @ViewBag.callus.Adress</p><!-- Edit -->
                    <p><i class="fa fa-phone" aria-hidden="true"></i> <span>Phone:</span>  @ViewBag.callus.Phone</p><!-- Edit -->
                    <p><i class="fa fa-envelope" aria-hidden="true"></i> <span>Email:</span> <a href="http://mehulp.ml/cdn-cgi/l/email-protection" class="__cf_email__" data-cfemail="84f4e1f6f7ebeae5e8c4e3e9e5ede8aae7ebe9">[email&#160;protected]</a></p><!-- Edit -->
                </div>
                <div class="col-xs-12 col-md-3 col-lg-3 single-footer">
                    <h3><a href="#">Ex<span class="contrast">press</span>o</a></h3>
                    <p class="mb-20">@ViewBag.ExpressoFooter.Title </p><!-- Edit description -->
                    <h3>Follow Us</h3>
                    <a href="@ViewBag.ExpressoFooter.facebook_link" class="facebook-tooltip"><i class="fa fa-facebook"></i><span class="facebook-text">Facebook</span></a>
                    <a href="@ViewBag.ExpressoFooter.twitter_link" class="twitter-tooltip"><i class="fa fa-twitter"></i><span class="twitter-text">Twitter</span></a>
                    <a href="@ViewBag.ExpressoFooter.instagram_link" class="instagram-tooltip"><i class="fa fa-instagram"></i><span class="instagram-text">Instagram</span></a>
                    <a href="@ViewBag.ExpressoFooter.youtube_link" class="youtube-tooltip"><i class="fa fa-youtube"></i><span class="youtube-text">YouTube</span></a>
                    <a href="@ViewBag.ExpressoFooter.linkedin_link" class="linkedin-tooltip"><i class="fa fa-linkedin"></i><span class="linkedin-text">LinkedIn</span></a>
                    <a href="@ViewBag.ExpressoFooter.pinterest_link" class="pinterest-tooltip"><i class="fa fa-pinterest"></i><span class="pinterest-text">Pinterest</span></a>
                </div>
            </div>
            <div class="separator"></div>
            <div class="copyright">
                <p>&copy; 2017 <strong>Expresso</strong>. All rights reserved.</p>
            </div>
        </div>
    </footer>
    <!--=============== End Of Footer ===============-->
    
    <!-- ========== Scroll to Top ========== -->
    <div id="scroll-up">
        <i class="fa fa-arrow-circle-up"></i>
    </div>
    <!-- ========== End of Scroll to Top ========== -->
    <!-- Global site tag (gtag.js) - Google Analytics -->
    <script src="~/Public/cdn-cgi/scripts/d07b1474/cloudflare-static/email-decode.min.js"></script>
    <script async src="https://www.googletagmanager.com/gtag/js?id=UA-104518787-7"></script>
    <script>
        window.dataLayer = window.dataLayer || [];
        function gtag() { dataLayer.push(arguments); }
        gtag('js', new Date());
        gtag('config', 'UA-104518787-7');
    </script>
    <!-- jQuery -->
    <script src="~/Public/vendor/jquery/jquery-2.1.4.min.js"></script>
    <!-- Stellar js -->
    <script src="~/Public/vendor/stellar/jquery.stellar.min.js"></script>
    <!-- plugin js -->
    <script src="~/Public/vendor/plugin/plugin.js"></script>
    <!-- Mixitup -->
    <script src="~/Public/vendor/mixitup/mixitup.min.js"></script>
    <!-- Waypoint -->
    <script src="~/Public/vendor/waypoint/waypoint-2.0.3.min.js"></script>
    <!-- Counterup -->
    <script src="~/Public/vendor/counter/counterup.min.js"></script>
    <!-- Form Validation -->
    <script src="~/Public/vendor/validate/validate.js"></script>
    <!-- WOW -->
    <script src="~/Public/vendor/animate/wow.min.js"></script>
    <!-- Google Map -->
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBC2oOenaQAs2ImrzwUz3xk8RCM_0_KiZA"></script>
    <!-- Main js -->
    <script src="~/Public/expresso/js/main.js"></script>

</body>
<!-- ========== End of Body ========== -->
</html>  